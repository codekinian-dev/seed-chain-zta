# Keycloak PostgreSQL Database Configuration
POSTGRES_DB=keycloak
POSTGRES_USER=keycloak
POSTGRES_PASSWORD=YOUR_POSTGRES_PASSWORD_HERE
POSTGRES_PORT=6432
PGDATA=/var/lib/postgresql/data/pgdata

# Keycloak Admin Configuration
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=YOUR_ADMIN_PASSWORD_HERE
KEYCLOAK_HTTP_PORT=6080

# Keycloak Database Connection
KC_DB=postgres
KC_DB_URL_HOST=auth_keycloak_postgres
KC_DB_URL_PORT=6432
KC_DB_DATABASE=keycloak
KC_DB_USERNAME=keycloak
KC_DB_PASSWORD=YOUR_POSTGRES_PASSWORD_HERE

# Keycloak Server Configuration
KC_HTTP_ENABLED=true
KC_HOSTNAME_STRICT=false
KC_HOSTNAME_STRICT_HTTPS=false
KC_PROXY=edge
KC_PROXY_HEADERS=xforwarded
KC_SSL_REQUIRED=none
