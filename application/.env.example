# Environment Configuration
NODE_ENV=development
PORT=3001

# Keycloak Configuration
KEYCLOAK_URL=http://localhost:6080
KEYCLOAK_REALM=SeedCertificationRealm
KEYCLOAK_CLIENT_ID=seed-cert-frontend
KEYCLOAK_CLIENT_SECRET=
KEYCLOAK_PUBLIC_KEY=

# IPFS Cluster Configuration
IPFS_API_URL=http://localhost:9094
IPFS_GATEWAY_URL=http://localhost:9090/ipfs
IPFS_UPLOAD_TIMEOUT=120000
IPFS_MAX_RETRIES=3

# Hyperledger Fabric Configuration
FABRIC_NETWORK_PATH=./config/connection-profile.json
FABRIC_CHANNEL=benihchannel
FABRIC_CONTRACT=benih-certification
FABRIC_WALLET_PATH=./wallet
FABRIC_USER_ID=appUser
FABRIC_MSP_ID=BPSBPBenihMSP

# Logging Configuration
LOG_LEVEL=info
LOG_DIR=./logs

# Upload Configuration
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/jpg,application/pdf,application/vnd.openxmlformats-officedocument.wordprocessingml.document

# Queue Configuration (Bull/Redis)
REDIS_HOST=localhost
REDIS_PORT=6379
QUEUE_NAME=ipfs-upload-queue

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:8080,http://localhost:3000
